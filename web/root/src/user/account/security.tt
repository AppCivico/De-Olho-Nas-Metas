	   <section class="metas-detail profile-timeline">
		<div class="container">

			<div class="row">


					<br /><br />
				<div class="col-sm-9 .col-md.offset-4">
					<form action="/form/change_password/[%c.user.id%]" method="POST">
							<div class="row">
								<div class="col-md-4 col-md-offset-3">
									[%IF error_msg %]<div class="alert alert-danger"><p>[%error_msg%]</p></div>[%END%]
							        [%IF status_msg %]<div class="alert alert-success"><p>[%status_msg%]</p></div>[%END%]
								</div>
							</div>
							<div class="row">
								<div class="col-md-4 col-md-offset-4">
									[%input_text('current_password','Senha Atual',type => 'password' )|none%]
								</div>
							</div>
							<div class="row">
								<div class="col-md-4 col-md-offset-4">
									[%input_text('password','Nova Senha',type => 'password' )|none%]
								</div>
							</div>
							<div class="row">
								<div class="col-md-4 col-md-offset-4">
									[%input_text('password_confirm','Confirme Nova Senha',type => 'password' )|none%]
								</div>
							</div>
							<div class="row">
								<div class="col-md-4 col-md-offset-7">
									<input type="submit" class="btn"/>
								</div>
							</div>
					</form>

					<br /><br />
				</div>
				<div class="col-sm-3">
					<div class="profile-box text-center">
						<div class="img text-center"><img src="/static/images/user/[%c.user.id%]/[%c.user.id%].jpg" border="0" class="img-responsive"></div>
						<div class="name text-center">[% c.user.name %]</div>
						<button class="btn btn-yellow"><a href="/user/perfil/editar">Editar perfil</a></button>
					</div>
					
					<div class="list-box list-brown">
						<ul>
							<li><a href="">Seguindo</a></li>
